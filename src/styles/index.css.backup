.wrapper{
    width:70%;
    display:grid;
    margin-left:15%;
}
html{
    background-color: black;
}
ul {
    list-style-type: none;
}
a {
    text-decoration: none;
    color:white;
}
.header{
    text-align: center;
    font-family:"Silkscreen";
    font-size:30px;
    color:white;
    width:1000px;
    display:block;
    margin-left:auto;
    margin-right:auto;
}
.header img {
    height:100px;
    display:inline-block;
    padding:10px 0 10px 0;
    vertical-align:middle;
}

.header h3 {
    display:inline-block;
    vertical-align:middle;
}
.content{
    display:grid;
    grid-template-columns: 150px 850px;
    gap:20px;
    font-family: "Rajdhani";
    color:white;
}

.right-main{
    display:grid;
    grid-template-columns: 2fr 1fr;
    gap:20px;

}
.box{
    background-color:#363838;
}

.box h4{
    text-align:center;
    background-color:#999999;
    color:black;
    font-family:'Silkscreen';
    font-size:15px;
    padding: 0.5vw 0 0.5vw 0;
}
.left-links-li{
    padding: 0 10% 10% 10%;
}
.left-links-li li {
    padding:5%;
}

.self-icn img{
    height:40em;
    padding-top: 5%;
}

.post-list li {
    padding:0 0 5% 0; 
}

.text-post {
    padding:0 2% 2% 2%;
}
.text-post a {
    color:#c75c04;
}

